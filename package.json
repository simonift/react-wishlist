{
  "name": "wishlist",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "lint": "eslint --fix src/**/*.{js,jsx}",
    "format": "prettier --write \"*.{js, jsx, json, css}\""
  },
  "keywords": [
    "parcel",
    "react"
  ],
  "author": "Simón Faúndez Torres",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonift/react-wishlist.git"
  },
  "description": "Pruebas con react",
  "dependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "10.5.4",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint"
    ],
    "src/**/*,{js,jsx}": [
      "prettier -- write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "aribnb",
    "env": {
      "browser": true
    },
    "rules": {
      "linebreak-style": "off"
    }
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
